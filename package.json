{
  "name": "node-express-api",
  "version": "1.0.0",
  "description": "Api server",
  "license": "MIT",
  "scripts": {
    "dev": "node_modules/.bin/nodemon --watch server index.js",
    "start:live": "pm2-docker start process.json --env production",
    "test": "NODE_ENV=test node_modules/.bin/jest",
    "coverage": "NODE_ENV=test node_modules/.bin/jest --coverage",
    "start": "npm run dev"
  },
  "keywords": [
    "express-api, docker, docker-compose",
    "pm2"
  ],
  "author": "Juan Heber",
  "dependencies": {
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "config": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html"
    ],
    "collectCoverageFrom": [
      "server/**/*.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/server/test/setup.js",
    "testRegex": "test/.*\\.test\\.js$"
  }
}
